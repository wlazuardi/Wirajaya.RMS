                CssClass="btn btn-primary pull-right" OnClick="lbAddInterview_Click">
                <i class="fa fa-plus"></i>&nbsp; Tambahkan Data Interview
            </asp:LinkButton>
        </div>
    </asp:Panel>
    
    <asp:Panel ID="pnlKandidatFinal" runat="server">
        <div class="modal-body">
            <asp:Repeater ID="rptKandidatFinal" runat="server" OnItemDataBound="rptKandidatFinal_ItemDataBound">
                <HeaderTemplate>
                    <table class="table table-stripped">
                        <tr>
                            <th>#</th>
                            <th>Nama</th>
                            <th>Gender</th>
                            <th>No. Identitas</th>
                            <th>No. Handphone</th>                                                    
                            <th>E-mail</th>
                            <th style="width:100px;">Download CV</th>
                            <th>Pilih</th>                            
                        </tr>
                </HeaderTemplate>
                <ItemTemplate>
                        <tr>
                            <td><asp:Label ID="lblNo" runat="server"></asp:Label></td>
                            <td>
                                <asp:Label ID="lblNama" runat="server"></asp:Label>
                                <asp:TextBox ID="hidKdKandidat" Visible="false" runat="server"></asp:TextBox>
                            </td>
                            <td><asp:Label ID="lblGender" runat="server"></asp:Label></td>
                            <td><asp:Label ID="lblNoIdentitas" runat="server"></asp:Label></td>
                            <td><asp:Label ID="lblNoHandphone" runat="server"></asp:Label></td>                                                    
                            <td><asp:Label ID="lblEmail" runat="server"></asp:Label></td>                                                    
                            <td align="center">
                                <asp:HyperLink ID="linkDownloadCV" runat="server" CssClass="btn btn-success" ToolTip="Download" Target="_blank">
                                    <i class="fa fa-download"></i>
                                </asp:HyperLink>
                            </td>
                            <td>
                                <asp:CheckBox ID="chkPilih" runat="server"/>
                            </td>
                        </tr>
                </ItemTemplate>
                <FooterTemplate>
                    </table>
                </FooterTemplate>
            </asp:Repeater>
            <div class="row">
                <div class="col-sm-12">
                    <uc:AlertControl ID="alertFinalisasi" runat="server" Visible="false" Dismissable="true" />                                    
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <asp:LinkButton ID="btnFinalisasi" runat="server"
                CssClass="btn btn-primary pull-right" OnClick="btnFinalisasi_Click">
                <i class="fa fa-check"></i>&nbsp; Finalisasi
            </asp:LinkButton>
        </div>
    </asp:Panel>
</asp:Content>
